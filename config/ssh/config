BatchMode yes
Compression yes
Protocol 2

Host *
    IdentityFile ~/dotfiles/private/ssh/id_rsa
    UserKnownHostsFile ~/dotfiles/private/ssh/known_hosts
    StrictHostKeyChecking no

Host github.com
    HostName github.com
    IdentityFile ~/dotfiles/private/ssh/github_rsa
    User git
#   LogLevel DEBUG3

Host gitlab.internal.unity3d.com
    HostName gitlab.internal.unity3d.com
    IdentityFile ~/dotfiles/private/ssh/gitlab_rsa
    User git
